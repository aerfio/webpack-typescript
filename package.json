{
	"name": "final-webpack",
	"version": "1.0.0",
	"description": "",
	"main": "src/index.tsx",
	"scripts": {
		"start": "webpack-dev-server --open --config=webpack.dev.js",
		"build": "webpack --config=webpack.prod.js",
		"lint-check": "tslint -c tslint.json -p . --format verbose && prettier --check 'src/**/*.{ts,tsx}'",
		"lint-fix": "tslint -c tslint.json -p . --format verbose --fix && prettier --write 'src/**/*.{ts,tsx}'",
		"conflict-check": "tslint-config-prettier-check ./tslint.json"
	},
	"browserslist": [
		"last 2 Chrome versions"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.{js,jsx}": [
			"prettier --write",
			"git add"
		],
		"**/*.{ts,tsx}": [
			"tslint -c tslint.json -p . --format verbose --fix",
			"prettier --write",
			"git add"
		]
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/plugin-proposal-class-properties": "^7.3.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"@types/react": "^16.8.6",
		"@types/react-dom": "^16.8.2",
		"@types/styled-components": "^4.1.11",
		"babel-loader": "^8.0.5",
		"babel-plugin-styled-components": "^1.10.0",
		"clean-webpack-plugin": "^1.0.1",
		"copy-webpack-plugin": "^5.0.0",
		"dotenv-webpack": "^1.7.0",
		"file-loader": "^3.0.1",
		"fork-ts-checker-webpack-plugin": "^0.5.2",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.5",
		"prettier": "^1.16.4",
		"react-dev-utils": "^7.0.3",
		"resource-hints-webpack-plugin": "0.0.2",
		"tslib": "^1.9.3",
		"tslint": "^5.13.1",
		"tslint-config-prettier": "^1.18.0",
		"tslint-react": "^3.6.0",
		"typescript": "^3.3.3333",
		"url-loader": "^1.1.2",
		"webpack": "^4.29.6",
		"webpack-bundle-analyzer": "^3.1.0",
		"webpack-cli": "^3.2.1",
		"webpack-dev-server": "^3.2.1",
		"webpack-merge": "^4.2.1"
	},
	"dependencies": {
		"react": "^16.8.3",
		"react-dom": "^16.8.3",
		"styled-components": "^4.1.3"
	}
}
